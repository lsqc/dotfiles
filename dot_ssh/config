# GitLab.com
Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

# GitHub.com
Host github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

Host git.ha.si
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

Host git.lsdevcloud.net
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey
